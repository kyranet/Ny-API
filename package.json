{
  "name": "ny-api",
  "version": "3.0.0",
  "description": "NyProject API Microservice",
  "main": "dist/src/NyAPI.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json src/**/*.ts --fix",
    "pm2:delete": "pm2 delete API",
    "pm2:logs": "pm2 logs API --lines=200",
    "pm2:restart": "pm2 restart API",
    "pm2:start-harmony": "pm2 start dist/src/NyAPI.js --node-args=\"--harmony\" --name=\"API\"",
    "pm2:start": "pm2 start dist/src/NyAPI.js --name=\"API\"",
    "pm2:stop": "pm2 stop API",
    "start": "node dist/src/NyAPI.js",
    "test": "tslint -p tsconfig.json src/**/*.ts",
    "update": "yarn",
    "compile": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kyranet/Ny-API.git"
  },
  "keywords": [
    "Web API",
    "REST API",
    "NyProject"
  ],
  "contributors": [
    "kyranet"
  ],
  "private": true,
  "author": "kyranet <kyradiscord@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/kyranet/Ny-API/issues"
  },
  "homepage": "https://github.com/kyranet/Ny-API#readme",
  "dependencies": {
    "fs-nextra": "^0.4.2",
    "node-fetch": "^2.3.0",
    "veza": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^11.9.0",
    "@types/node-fetch": "^2.1.4",
    "eslint-config-klasa": "github:dirigeants/klasa-lint",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
